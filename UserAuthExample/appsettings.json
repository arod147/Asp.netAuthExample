{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  // Update this setting to send emails using smtp here I am using outlook
  "EmailConfiguration": {
    "From": "myEmail@outlook.com", //Here
    "SmtpServer": "smtp-mail.outlook.com",
    "Port": 587,
    "Username": "myEmail@outlook.com", //Here
    "Password": "myPassword" //Here
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    // Change to your sql connection string
    "DefualtConnection": "Server=serverName;Database=coolDB;Trusted_Connection=True" //Here
  }
}
